<% provide(:title, "Team Overview") %>
<h1>Team Overview</h1>
<p>
Displays all current students for an admin to make changes to
</p>

<ol>
<% @student.each do |student| %>
  <li>
    <%= student.fname %>
    <%= student.lname %>
    <%= student.team_id %>
    <%= student.id %>
    <%= link_to "Edit", edit_student_path(student) %>
  </li>

<% end %>
</ol>
